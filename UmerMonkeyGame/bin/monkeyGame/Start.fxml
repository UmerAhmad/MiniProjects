<?xml version="1.0" encoding="UTF-8"?>

<!-- Importing all necessities for fxml -->
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<!-- Main block in which everything is constructed, border pane, grid, button work, text field, and label-->
<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="monkeyGame.StartController">
   <center>
      <Pane prefHeight="609.0" prefWidth="804.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="190.0" fitWidth="433.0" layoutX="98.0" layoutY="28.0">
               <image>
                  <Image url="images/monkeytitle.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="139.0" fitWidth="388.0" layoutX="113.0" layoutY="180.0">
               <image>
                  <Image url="images/bononzatitle.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="429.0" fitWidth="294.0" layoutX="510.0" layoutY="50.0">
               <image>
                  <Image url="images/treetitle.png" />
               </image>
            </ImageView>
            <ImageView layoutX="575.0" layoutY="427.0">
               <image>
                  <Image url="images/asteroid.png" />
               </image>
            </ImageView>
            <Button layoutX="211.0" layoutY="339.0" mnemonicParsing="false" onAction="#buttonClickHandler" prefHeight="65.0" prefWidth="152.0" text="Play" />
            <Button layoutX="211.0" layoutY="420.0" mnemonicParsing="false" onAction="#buttonClickHandler" prefHeight="65.0" prefWidth="152.0" text="Instructions" />
            <Button layoutX="211.0" layoutY="501.0" mnemonicParsing="false" onAction="#closeCurrentWindow" prefHeight="65.0" prefWidth="152.0" text="Quit" />
         </children>
      </Pane>
   </center>
</BorderPane>
